  "version": "0.2.0",
  "configurations": [

  {
    "name": "Tests",
    "args": [
      "--extensionDevelopmentPath=${workspaceFolder}",
      "--disable-extensions",
      "--extensionTestsPath=${workspaceRoot}/out-tests-bundle/testsEntryPoint.js",
    ],
    "outFiles": [
      "${workspaceFolder}/out-tests-bundle/**/*.js"
    ],
    "request": "launch",
    "runtimeExecutable": "${execPath}",
    "type": "pwa-extensionHost",
    //"console": "integratedTerminal",
    //"outputCapture": "std",
  },


  {
    "name": "Extension",
    "args": [
      "--extensionDevelopmentPath=${workspaceFolder}",
   //   "--disable-extensions"
    ],
    "outFiles": [
      "${workspaceFolder}/out-bundle/**/*.js"
    ],
    "request": "launch",
    "runtimeExecutable": "${execPath}",
    "type": "pwa-extensionHost"
  }


  ]
}
