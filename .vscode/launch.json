  "version": "0.2.0",
  "configurations": [

  {
    "name": "Launch Extension Tests",
    "args": [
      "--extensionDevelopmentPath=${workspaceFolder}",
      "--disable-extensions",
      "--extensionTestsPath=${workspaceRoot}/dev/out/test/suite/index.js",
    ],
    "outFiles": [
      "${workspaceFolder}/out/**/*.js"
    ],
    "request": "launch",
    "runtimeExecutable": "${execPath}",
    "type": "pwa-extensionHost"
  },


  {
    "name": "Launch Extension",
    "args": [
      "--extensionDevelopmentPath=${workspaceFolder}",
   //   "--disable-extensions"
    ],
    "outFiles": [
      "${workspaceFolder}/out-bundle/**/*.js"
    ],
    "request": "launch",
    "runtimeExecutable": "${execPath}",
    "type": "pwa-extensionHost"
  }


  ]
}
